<service
		url="http://reactomews.oicr.on.ca:8080/ReactomeRESTfulAPI/RESTfulWS/biopaxExporter" 
		media-type="text/plain"
		documentation="http://reactomews.oicr.on.ca:8080/ReactomeRESTfulAPI/ReactomeRESTFulAPI.html">

		<static-attribute name="Level2" type="String" value="Level2" attribute-encoding="doubleslash" attribute-encoding-index="1"/>
		<static-attribute name="thing1" type="Integer" value="109581" attribute-encoding="doubleslash" attribute-encoding-index="2"/>

	<access-method type="FREE" name="reactome_species_1" cost="1.0">

		<attribute name="ORGANISM" type="String" output="true" relation-attribute="ORGANISM"/>
		<attribute name="NAME" type="String" output="true" relation-attribute="NAME"/>
		<attribute name="SHORT-NAME" type="String" output="true" relation-attribute="SHORT-NAME"/>
		<attribute name="XREF" type="String" output="true" relation-attribute="XREF"/>
		<attribute name="COMMENT" type="String" output="true" relation-attribute="COMMENT"/>
	</access-method>
</service>
