					<variable name="pname" />
					<variable name="pmfgr" />
					<variable name="pbrand" />
					<variable name="ptype" />
					<variable name="psize" />
					<variable name="pcontainer" />
					<variable name="price" />
					<variable name="pcomment" />
				</atom>
			</head>
		</dependency>
		<dependency>
			<body>
				<atom name="orders">
					<variable name="orderkey" />
					<variable name="ckey" />
					<variable name="ostatus" />
					<variable name="price" />
