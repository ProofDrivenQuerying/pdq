<?xml version="1.0" encoding="UTF-8"?>
<schema>
	<sources>
		<source name="tpch" discoverer="uk.ac.ox.cs.pdq.sql.PostgresqlSchemaDiscoverer"
			url="jdbc:postgresql://localhost/" database="tpch_0001" username="root"
			password="root" />
	</sources>
	<relations>
		<relation name="customer" source="tpch">
		</relation>
		<relation name="part" source="tpch">
		</relation>
		<relation name="supplier" source="tpch">
		</relation>
		<relation name="partsupp" source="tpch">
		</relation>
		<relation name="nation" source="tpch">
		</relation>
		<relation name="region" source="tpch">
		</relation>
		<relation name="orders" source="tpch">
		</relation>
		<relation name="lineitem" source="tpch">
		</relation>
		<!-- <relation name="v1" source="tpch"> </relation> <relation name="v2" 
			source="tpch"> </relation> -->

		<view name="v1">
			<attribute name="okey" type="java.lang.Integer" />
			<attribute name="llinenumber" type="java.lang.Integer" />
		</view>

		<view name="v2">
			<attribute name="ckey" type="java.lang.Integer" />
			<attribute name="nkey" type="java.lang.Integer" />
		</view>

	</relations>
	<dependencies>
		<!-- CREATE VIEW V1 WITH SCHEMABINDING AS SELECT ORDERS.O_ORDERKEY, LINEITEM.L_LINENUMBER 
			FROM dbo.ORDERS, dbo.LINEITEM WHERE ORDERS.O_ORDERKEY = LINEITEM.L_ORDERKEY 
			AND LINEITEM.L_COMMITDATE=LINEITEM.L_RECEIPTDATE AND LINEITEM.L_LINESTATUS 
			= 'O'; -->
		<dependency>
			<body>
				<atom name="v1">
					<variable name="okey" />
					<variable name="llinenumber" />
				</atom>
			</body>
			<head>
				<atom name="orders">
					<variable name="okey" />
					<variable name="ckey" />
					<variable name="ostatus" />
					<variable name="price" />
					<variable name="orderdate" />
					<variable name="opriority" />
					<variable name="oclerk" />
					<variable name="oshipprio" />
					<variable name="ocomment" />
				</atom>
				<atom name="lineitem">
					<variable name="okey" />
					<variable name="lpartkey" />
					<variable name="lsuppkey" />
					<variable name="llinenumber" />
					<variable name="lquantity" />
					<variable name="lextendedprice" />
					<variable name="ldiscount" />
					<variable name="ltax" />
					<variable name="lreturnflag" />
					<constant value="O" />
					<variable name="date" />
					<variable name="date" />
					<variable name="lreceiptdate" />
					<variable name="lshipinstruct" />
					<variable name="lshipmode" />
					<variable name="lcomment" />
				</atom>
			</head>
		</dependency>
		<dependency>
			<body>
				<atom name="orders">
					<variable name="okey" />
					<variable name="ckey" />
					<variable name="ostatus" />
					<variable name="price" />
					<variable name="orderdate" />
					<variable name="opriority" />
					<variable name="oclerk" />
					<variable name="oshipprio" />
					<variable name="ocomment" />
				</atom>
				<atom name="lineitem">
					<variable name="okey" />
					<variable name="lpartkey" />
					<variable name="lsuppkey" />
					<variable name="llinenumber" />
					<variable name="lquantity" />
					<variable name="lextendedprice" />
					<variable name="ldiscount" />
					<variable name="ltax" />
					<variable name="lreturnflag" />
					<constant value="O" />
					<variable name="date" />
					<variable name="date" />
					<variable name="lreceiptdate" />
					<variable name="lshipinstruct" />
					<variable name="lshipmode" />
					<variable name="lcomment" />
				</atom>
			</body>
			<head>
				<atom name="v1">
					<variable name="okey" />
					<variable name="llinenumber" />
				</atom>
			</head>
		</dependency>
		<!-- CREATE VIEW V2 WITH SCHEMABINDING AS SELECT CUSTOMER.C_CUSTKEY, NATION.N_NATIONKEY 
			FROM dbo.NATION, dbo.CUSTOMER WHERE NATION.N_NATIONKEY = CUSTOMER.C_NATIONKEY; -->
		<dependency>
			<body>
				<atom name="v2">
					<variable name="ckey" />
					<variable name="nkey" />
				</atom>
			</body>
			<head>
				<atom name="nation">
					<variable name="nkey" />
					<variable name="nname" />
					<variable name="rkey" />
					<variable name="ncomment" />
				</atom>
				<atom name="customer">
					<variable name="ckey" />
					<variable name="cname" />
					<variable name="caddress" />
					<variable name="nkey" />
					<variable name="cphone" />
					<variable name="cacctbal" />
					<variable name="cmktsegment" />
					<variable name="ccomment" />
				</atom>
			</head>
		</dependency>
		<dependency>
			<body>
				<atom name="nation">
					<variable name="nkey" />
					<variable name="nname" />
					<variable name="rkey" />
					<variable name="ncomment" />
				</atom>
				<atom name="customer">
					<variable name="ckey" />
					<variable name="cname" />
					<variable name="caddress" />
					<variable name="nkey" />
					<variable name="cphone" />
					<variable name="cacctbal" />
					<variable name="cmktsegment" />
					<variable name="ccomment" />
				</atom>
			</body>
			<head>
				<atom name="v2">
					<variable name="ckey" />
					<variable name="nkey" />
				</atom>
			</head>
		</dependency>
	</dependencies>
</schema>
