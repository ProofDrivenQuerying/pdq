<?xml version="1.0" encoding="UTF-8"?>
<schema>
	<sources>
		<source name="tpch" discoverer="uk.ac.ox.cs.pdq.sql.PostgresqlSchemaDiscoverer"
			url="jdbc:postgresql://localhost/" database="tpch_0001" username="root"
			password="root" />
	</sources>
	<relations>
		<relation name="customer" source="tpch">
		</relation>
		<relation name="part" source="tpch">
		</relation>
		<relation name="supplier" source="tpch">
		</relation>
		<relation name="partsupp" source="tpch">
		</relation>
		<relation name="nation" source="tpch">
		</relation>
		<relation name="region" source="tpch">
		</relation>
		<relation name="orders" source="tpch">
		</relation>
		<relation name="lineitem" source="tpch">
		</relation>
		<!-- <relation name="v1" source="tpch"> </relation> <relation name="v2" 
			source="tpch"> </relation> -->

		<view name="v1">
			<attribute name="okey" type="java.lang.Integer" />
			<attribute name="pkey" type="java.lang.Integer" />
		</view>

		<view name="v2">
			<attribute name="ckey" type="java.lang.Integer" />
		</view>

	</relations>
	<dependencies>

		<!-- CREATE VIEW v1 AS SELECT ORDERS.O_ORDERKEY, PART.P_PARTKEY FROM ORDERS, 
			PART WHERE PART.P_RETAILPRICE=ORDERS.O_TOTALPRICE AND PART.P_MFGR='Manufacturer#1'; -->
		<dependency>
			<body>
				<atom name="v1">
					<variable name="okey" />
					<variable name="pkey" />
				</atom>
			</body>
			<head>

				<atom name="part">
					<variable name="pkey" />
					<variable name="pname" />
					<constant value="Manufacturer1" />
					<variable name="pbrand" />
					<variable name="ptype" />
					<variable name="psize" />
					<variable name="pcontainer" />
					<variable name="price" />
					<variable name="pcomment" />
				</atom>

				<atom name="orders">
					<variable name="okey" />
					<variable name="ckey" />
					<variable name="ostatus" />
					<variable name="price" />
					<variable name="orderdate" />
					<variable name="opriority" />
					<variable name="oclerk" />
					<variable name="oshipprio" />
					<variable name="ocomment" />
				</atom>
			</head>
		</dependency>
		<dependency>
			<body>
				<atom name="part">
					<variable name="pkey" />
					<variable name="pname" />
					<constant value="Manufacturer1" />
					<variable name="pbrand" />
					<variable name="ptype" />
					<variable name="psize" />
					<variable name="pcontainer" />
					<variable name="price" />
					<variable name="pcomment" />
				</atom>

				<atom name="orders">
					<variable name="okey" />
					<variable name="ckey" />
					<variable name="ostatus" />
					<variable name="price" />
					<variable name="orderdate" />
					<variable name="opriority" />
					<variable name="oclerk" />
					<variable name="oshipprio" />
					<variable name="ocomment" />
				</atom>
			</body>
			<head>
				<atom name="v1">
					<variable name="okey" />
					<variable name="pkey" />
				</atom>
			</head>
		</dependency>

		<!-- CREATE VIEW V3 WITH SCHEMABINDING AS SELECT CUSTOMER.C_CUSTKEY FROM 
			dbo.CUSTOMER WHERE CUSTOMER.C_MKTSEGMENT='AUTOMOBILE' AND CUSTOMER.C_PHONE 
			LIKE '10-%'; -->
		<dependency>
			<body>
				<atom name="v2">
					<variable name="ckey" />
				</atom>
			</body>
			<head>
				<atom name="customer">
					<variable name="ckey" />
					<variable name="cname" />
					<variable name="caddress" />
					<variable name="nkey" />
					<constant value="10" />
					<variable name="cacctbal" />
					<constant value="AUTOMOBILE" />
					<variable name="ccomment" />
				</atom>
			</head>
		</dependency>
		<dependency>
			<body>
				<atom name="customer">
					<variable name="ckey" />
					<variable name="cname" />
					<variable name="caddress" />
					<variable name="nkey" />
					<constant value="10" />
					<variable name="cacctbal" />
					<constant value="AUTOMOBILE" />
					<variable name="ccomment" />
				</atom>
			</body>
			<head>
				<atom name="v2">
					<variable name="ckey" />
				</atom>
			</head>
		</dependency>
	</dependencies>
</schema>
