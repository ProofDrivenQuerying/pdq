<?xml version="1.0" encoding="UTF-8"?>
<query>
	<!-- Supply cost of parts available in quantity 10 -->
	<body>
		<atom name="part">
			<variable name="pkey" />
			<variable name="pname" />
			<constant value="Manufacturer1" />
			<variable name="pbrand" />
			<variable name="ptype" />
			<variable name="psize" />
			<variable name="pcontainer" />
			<variable name="price" />
			<variable name="pcomment" />
		</atom>

		<atom name="orders">
			<variable name="okey" />
			<variable name="ckey" />
			<variable name="ostatus" />
			<variable name="price" />
			<variable name="orderdate" />
			<variable name="opriority" />
			<variable name="oclerk" />
			<variable name="oshipprio" />
			<variable name="ocomment" />
		</atom>

		<atom name="customer">
			<variable name="ckey" />
			<variable name="cname" />
			<variable name="caddress" />
			<variable name="nkey" />
			<constant value="10" />
			<variable name="cacctbal" />
			<constant value="AUTOMOBILE" />
			<variable name="ccomment" />
		</atom>
	</body>
	<head name="Q">
		<variable name="okey" />
		<variable name="pkey" />
		<variable name="ckey" />
	</head>
</query>
