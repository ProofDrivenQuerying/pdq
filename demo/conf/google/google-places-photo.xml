<service>
	<name>Google Places Photos</name>
	<documentation>https://developers.google.com/places/documentation/photos</documentation>
	<usage-limits>
		<!-- 
		<pricing>free</pricing>
		<frequency max="2500" period="24h"/>
		 -->
		<api-key required="true"/>
	</usage-limits>
	<protocol type="REST" baseurl="https://maps.googleapis.com/maps/api/place/photo?">
		<input-params>
			<param name="key"            type="string"          required="true"/>
			<param name="photoreference" type="place_reference" required="true"/>
			<param name="sensor"         type="boolean"         required="true"/>
			<param name="maxheight"      type="integer"         required="true"/>
		</input-params>
		<input-params>
			<param name="key"            type="string"          required="true"/>
			<param name="photoreference" type="place_reference" required="true"/>
			<param name="sensor"         type="boolean"         required="true"/>
			<param name="maxwidth"      type="integer"         required="true"/>
		</input-params>
		<output-params>
			<param name="result"         type="image"/>
		</output-params>
	</protocol>
</service>