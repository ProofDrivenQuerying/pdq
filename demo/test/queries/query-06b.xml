<?xml version="1.0" encoding="UTF-8"?>
<query>
	<!--
	Elevation of capital cities of low income countries, and % of people living
	in low level areas. (not answerable without the dependencies) 
	 -->
	<body>
		<atom name="GoogleElevation">
			<variable name="capLat" />
			<variable name="capLng" />
			<variable name="elevation" />
			<variable name="resolution" />
		</atom>
		<atom name="WBData">
			<variable name="iso"/>
			<variable name="indicator"/>
			<constant value="2010"/>
			<variable name="countryName"/>
			<variable name="value"/>
		</atom>
		<atom name="WBIndicators">
			<variable name="indicator" />
			<variable name="topicId" />
			<constant value="Population living in areas where elevation is below 5 meters (% of total population)" />
			<variable name="sourceId" />
			<variable name="sourceNote" />
		</atom>
		<atom name="WBIncomeLevels">
			<variable name="incomeLevel" />
			<constant value="Low income" />
		</atom>
		<atom name="WBCountries">
			<variable name="incomeLevel"/>
			<variable name="iso"/>
			<variable name="name"/>
			<variable name="incomeLevelLabel"/>
			<variable name="lendingType"/>
			<variable name="capitalCity"/>
			<variable name="capLat"/>
			<variable name="capLng"/>
		</atom>
	</body>
	<head name="Q">
		<variable name="name" />
		<variable name="value" />
		<variable name="capitalCity" />
		<variable name="elevation" />
	</head>
</query>
