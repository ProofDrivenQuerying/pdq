<?xml version="1.0" encoding="UTF-8"?>
<query name="Example 1" description="" type="conjunctive">
	<body>

		<atom name="Citation">
			<variable name="cit_id" />
			<variable name="cit_source" />
			<variable name="cit_citationType" />
			<variable name="cit_title" />
			<variable name="cit_authorString" />
			<variable name="cit_journalAbbreviation" />
			<variable name="cit_pubYear" />
			<variable name="cit_journalVolume" />
			<variable name="cit_issue" />
			<variable name="cit_pageInfo" />
			<variable name="cit_citedByCount" />
			<constant value="MED" />
			<variable name="id" />
		</atom>

		<atom name="Publication">
			<variable name="id" />
			<variable name="source" />
			<variable name="pmid" />
			<variable name="pmcid" />
			<variable name="DOI" />
			<variable name="title" />
			<variable name="authorString" />
			<variable name="journalTitle" />
			<variable name="issue" />
			<variable name="journalVolume" />
			<variable name="pubYear" />
			<variable name="journalIssn" />
			<variable name="pubType" />
			<variable name="inEPMC" />
			<variable name="inPMC" />
			<variable name="citedByCount" />
			<variable name="hasReferences" />
			<variable name="hasTextMinedTerms" />
			<variable name="hasDbCrossReferences" />
			<variable name="hasLabsLinks" />
			<variable name="hasTMAccessionNumbers" />
			<variable />
			<variable />
			<variable />
			<variable name="accession" />
		</atom>

		<atom name="TargetComponent">
			<variable name="accession" />
			<variable name="component_id" />
			<constant value="PROTEIN" />
			<variable name="description" />
			<constant value="Bos taurus" />
			<!-- <variable name="organism" /> -->
			<variable name="protein_classification_id" />
			<variable name="sequence" />
			<variable name="tax_id" />
		</atom>

	</body>
	<head name="Q">

		<variable name="id" />
		<variable name="source" />
		<variable name="pmid" />

		<variable name="cit_id" />
		<variable name="cit_source" />
		<variable name="cit_citationType" />
		<variable name="cit_title" />
		<variable name="cit_authorString" />

		<variable name="accession" />
		<variable name="component_id" />
		<variable name="description" />
		
		<variable name="protein_classification_id" />
		<variable name="sequence" />
		<variable name="tax_id" />
	</head>

</query>
