<?xml version="1.0" encoding="UTF-8"?>
<!-- The schema is based on TPCH. Small relations (nation, region) have free access, while others required the primary key 
	as input. -->
<schema>
	<sources>
		<source name="tpch_001" discoverer="uk.ac.ox.cs.pdq.sql.MySQLSchemaDiscoverer" url="jdbc:mysql://localhost/"
			database="tpch_001" username="root" password="root" />
	</sources>
	<relations>
		<relation name="customer" source="tpch_001">
			<access-method name="m1" type="LIMITED" inputs="1" cost="5" />
		</relation>
		<relation name="part" source="tpch_001">
			<access-method name="m2" type="LIMITED" inputs="1" cost="5" />
		</relation>
		<relation name="supplier" source="tpch_001">
			<access-method name="m3" type="LIMITED" inputs="1" cost="2" />
		</relation>
		<relation name="partsupp" source="tpch_001">
			<access-method name="m4" type="LIMITED" inputs="1,2" cost="5" />
		</relation>
		<relation name="nation" source="tpch_001">
			<access-method name="m5" type="FREE" cost="15" />
		</relation>
		<relation name="region" source="tpch_001">
			<access-method name="m6" type="FREE" cost="5" />
		</relation>
		<relation name="orders" source="tpch_001">
			<access-method name="m7" type="LIMITED" inputs="1" cost="5" />
		</relation>
		<relation name="lineitem" source="tpch_001">
			<access-method name="m8" type="LIMITED" inputs="1" cost="30" />
			<access-method name="m9" type="LIMITED" inputs="2" cost="30" />
			<access-method name="m10" type="LIMITED" inputs="1,2" cost="20" />
			<access-method name="m11" type="LIMITED" inputs="4" cost="30" />
			<access-method name="m12" type="LIMITED" inputs="1,4" cost="20" />
			<access-method name="m13" type="LIMITED" inputs="2,4" cost="20" />
			<access-method name="m14" type="LIMITED" inputs="1,2,4" cost="10" />
		</relation>
	</relations>
</schema>
