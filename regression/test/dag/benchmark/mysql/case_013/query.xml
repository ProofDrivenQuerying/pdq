<?xml version="1.0" encoding="UTF-8"?>
<query>
	<!--
	    Customer/supply info for customers who have orders with parts
	    whose quantity is the same as the available quantity.
	  -->
	<body>
		<atom name="orders">
			<variable name="w" />
			<variable name="v" />
			<variable />
			<variable />
			<variable />
			<variable />
			<variable name="o_clerk"/>
			<variable />
			<variable />
		</atom>
		<atom name="lineitem">
			<variable name="w" />
			<variable name="y" />
			<variable name="z" />
			<variable />
			<variable name="x" />
			<variable />
			<variable />
			<variable />
			<variable />
			<variable />
			<variable />
			<variable />
			<variable />
			<variable />
			<variable />
			<variable />
		</atom>
		<atom name="part">
			<variable name="y" />
			<variable name="p_name" />
			<variable />
			<variable />
			<variable />
			<variable />
			<variable />
			<variable />
			<variable />
		</atom>
		<atom name="partsupp">
			<variable name="y" />
			<variable name="z" />
			<variable name="x" />
			<variable />
			<variable />
		</atom>
		<atom name="supplier">
			<variable name="z" />
			<variable name="s_name" />
			<variable />
			<variable />
			<variable />
			<variable />
			<variable />
		</atom>
		<atom name="customer">
			<variable name="v" />
			<variable name="c_name" />
			<variable name="c_address" />
			<variable name="n" />
			<variable />
			<variable />
			<variable />
			<variable />
		</atom>
		<atom name="nation">
			<variable name="n" />
			<constant value="UNITED STATES" />
			<variable />
			<variable />
		</atom>
	</body>
	<head name="Q">
		<variable name="c_name" />
		<variable name="c_address" />
		<variable name="p_name" />
		<variable name="s_name" />
		<variable name="o_clerk" />
	</head>
</query>
