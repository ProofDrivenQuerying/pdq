<?xml version="1.0" encoding="UTF-8"?>
<query>
	<!--
		Customers from the United-States, with URGENT order of the same status.
		Cannot exploit the order_customer view. -->
	<body>
		<atom name="orders">
			<variable/>
			<variable name="custkey" />
			<variable name="orderstatus" />
			<variable />
			<variable />
			<constant value="1-URGENT" />
			<variable />
			<variable />
			<variable />
		</atom>
		<atom name="customer">
			<variable name="custkey" />
			<variable name="cname1" />
			<variable />
			<variable name="cnation" />
			<variable />
			<variable />
			<variable />
			<variable />
		</atom>
		<atom name="orders">
			<variable />
			<variable name="custkey2" />
			<variable name="orderstatus" />
			<variable />
			<variable />
			<constant value="1-URGENT" />
			<variable />
			<variable />
			<variable />
		</atom>
		<atom name="customer">
			<variable name="custkey2" />
			<variable name="cname2" />
			<variable />
			<variable name="cnation" />
			<variable />
			<variable />
			<variable />
			<variable />
		</atom>
		<atom name="nation">
			<variable name="cnation" />
			<constant value="UNITED STATES" />
			<variable />
			<variable />
		</atom>
	</body>
	<head name="Q">
		<variable name="cname1" />
		<variable name="cname2" />
		<variable name="orderstatus" />
	</head>
</query>
