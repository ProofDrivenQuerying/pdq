<?xml version="1.0" encoding="UTF-8"?>
<schema>
	<sources>
		<source name="tpch"
			discoverer="uk.ac.ox.cs.pdq.sql.PostgresqlSchemaDiscoverer"
			driver=org.postgresql.Driver
			url="jdbc:postgresql://localhost/" database="tpch_0001" username="root"
			password="root" />
	</sources>
	<relations>
		<relation name="customer" source="tpch">
			<access-method name="m1" type="FREE" cost="10" />
			<access-method name="m2" type="LIMITED" inputs="1"
				cost="5" />
			<access-method name="m3" type="LIMITED" inputs="4"
				cost="7" />
		</relation>
		<relation name="part" source="tpch">
			<access-method name="m4" type="FREE" cost="10" />
			<access-method name="m5" type="LIMITED" inputs="1,2"
				cost="5" />
		</relation>
		<relation name="supplier" source="tpch">
			<access-method name="m6" type="FREE" cost="20" />
			<access-method name="m7" type="LIMITED" inputs="1,2"
				cost="2" />
		</relation>
		<relation name="partsupp" source="tpch">
			<access-method name="m8" type="FREE" cost="15" />
			<access-method name="m9" type="LIMITED" inputs="1,3"
				cost="5" />
		</relation>
		<relation name="nation" source="tpch">
			<access-method name="m10" type="FREE" cost="5" />
			<access-method name="m11" type="LIMITED" inputs="2,3"
				cost="5" />
		</relation>
		<relation name="region" source="tpch">
			<access-method name="m12" type="FREE" cost="5" />
			<access-method name="m13" type="LIMITED" inputs="2"
				cost="5" />
		</relation>
		<relation name="orders" source="tpch">
			<access-method name="m14" type="FREE" cost="15" />
			<access-method name="m15" type="LIMITED" inputs="4"
				cost="5" />
			<access-method name="m16" type="LIMITED" inputs="8"
				cost="5" />
		</relation>
		<relation name="lineitem" source="tpch">
			<access-method name="m17" type="FREE" cost="25" />
			<access-method name="m18" type="LIMITED" inputs="8"
				cost="5" />
			<access-method name="m19" type="LIMITED" inputs="8,9"
				cost="5" />
		</relation>
	</relations>
</schema>
