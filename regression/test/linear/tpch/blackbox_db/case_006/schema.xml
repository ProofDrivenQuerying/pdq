<?xml version="1.0" encoding="UTF-8"?>
<schema>
	<sources>
		<source name="tpch"
			discoverer="uk.ac.ox.cs.pdq.sql.PostgresqlSchemaDiscoverer"
			url="jdbc:postgresql://localhost/" database="tpch_0001" username="root"
			password="root" />
	</sources>
	<relations>
		<relation name="customer" source="tpch">
			<access-method name="m1" type="LIMITED" inputs="4" />
		</relation>
		<relation name="part" source="tpch">
			<access-method name="m2" type="FREE" />
		</relation>
		<relation name="supplier" source="tpch">
			<access-method name="m3" type="LIMITED" inputs="4" />
		</relation>
		<relation name="partsupp" source="tpch">
			<access-method name="m4" type="LIMITED" inputs="1," />
			<access-method name="m5" type="LIMITED" inputs="2" />
			<access-method name="m6" type="LIMITED" inputs="1,2" />
		</relation>
		<relation name="nation" source="tpch">
			<access-method name="m7" type="LIMITED" inputs="3" />
		</relation>
		<relation name="region" source="tpch">
			<access-method name="m8" type="FREE" cost="5" />
		</relation>
		<relation name="orders" source="tpch">
			<access-method name="m9" type="LIMITED" inputs="2" />
		</relation>
		<relation name="lineitem" source="tpch">
			<access-method name="m10" type="LIMITED" inputs="2" />
			<access-method name="m11" type="LIMITED" inputs="3," />
			<access-method name="m12" type="LIMITED" inputs="2,3" />
		</relation>
	</relations>
</schema>
