<?xml version="1.0" encoding="UTF-8"?>
<query>
	<!--
	Population of France in 2000. (not answerable without dependencies) 
	 -->
	<body>
		<atom name="WBData">
			<variable name="iso"/>
			<variable name="indicator"/>
			<constant value="2000"/>
			<constant value="France"/>
			<variable name="value"/>
		</atom>
		<atom name="WBIndicators">
			<variable name="indicator" />
			<variable name="topicId" />
			<constant value="Population (Total)" />
			<variable name="sourceId" />
			<variable name="sourceNote" />
		</atom>
	</body>
	<head name="Q">
		<variable name="iso" />
		<variable name="value" />
	</head>
</query>
