<?xml version="1.0" encoding="UTF-8"?>
<query>
	<!--
	All neighbours of France. (answerable with/without dependencies) 
	 -->
	<body>
		<atom name="YahooPlaceRelationship">
			<constant value="neighbors"/>
			<variable name="place1"/>
			<variable name="place2"/>
			<variable name="x9"/>
			<variable name="x10"/>
			<variable name="x11"/>
		</atom>
		<atom name="YahooPlaceCode">
			<constant value="iso"/>
			<constant value="FR"/>
			<variable name="place1"/>
		</atom>
	</body>
	<head name="Q">
		<variable name="place2" />
	</head>
</query>
