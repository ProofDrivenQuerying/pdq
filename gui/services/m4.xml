<service
        name="m4"
        protocol="REST"
        url="http://pdq-webapp.cs.ox.ac.uk:8080/webapp/servlets/servlet/m4"
        media-type="application/xml"
        documentation=""
        result-delimiter="country"
        service-group="service-group-1">

    <policy name="1000_items_per_page" />

    <access-method type="FREE" name="m4" cost="1000.0" relation-name="part">
        <attribute name="p_partkey" type="Integer" output="true"  relation-attribute="p_partkey" path="p_partkey" />
        <attribute name="p_brand" type="String" output="true" relation-attribute="p_brand" path="p_brand"/>
        <attribute name="p_name" type="String" output="true" relation-attribute="p_name" path="p_name"/>
        <attribute name="p_size" type="Integer" output="true" relation-attribute="p_size" path="p_size"/>
        <attribute name="p_type" type="String" output="true" relation-attribute="p_type" path="p_type"/>
        <attribute name="p_retailprice" type="String" output="true" relation-attribute="p_retailprice" path="p_retailprice"/>
        <attribute name="p_comment" type="String" output="true" relation-attribute="p_comment" path="p_comment"/>
        <attribute name="p_mfgr" type="String" output="true" relation-attribute="p_mfgr" path="p_mfgr"/>
        <attribute name="p_container" type="String" output="true" relation-attribute="p_container" path="p_container"/>
    </access-method>
</service>